<% movie_ranks.each do |movie_rank| %>
 <div class="user-movie-rank-box">
  <h1><%= movie_rank.rank_status %></h1>
  <%= attachment_image_tag movie_rank.review.movie, :movie_image, size: "340x470", format: "jpg", fallback: "no_image.jpg" %>
  <% if current_user.id == movie_rank.user_id %>
    <%= link_to "削除", user_movie_rank_path(movie_rank), method: :delete, data: {confirm: "本当に削除してもよろしいですか？", "turbolinks" => false}, class: "btn-delete-shadow" %>
  <% end %><br/>
</div>
<% end %>


